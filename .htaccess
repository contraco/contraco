# CONTRACO DUPLICATE CONTENT FIX
# 301 Redirects from contraco_ prefixed URLs to canonical URLs
# Created: 2025-11-03

# Redirect contraco_ prefixed pages to canonical versions
Redirect 301 /contraco_about.html /about.html
Redirect 301 /contraco_ai_adoption_research.html /ai-adoption-research.html
Redirect 301 /contraco_ai_strategy_framework.html /ai-strategy-framework.html
Redirect 301 /contraco_cultural_adaptation_guide.html /cultural-adaptation-guide.html
Redirect 301 /contraco_digital_transformation_roi.html /digital-transformation-roi.html
Redirect 301 /contraco_insights.html /insights.html
Redirect 301 /contraco_leading_ai_transformation.html /leading-ai-transformation.html
Redirect 301 /contraco_organizational_design_ai.html /organizational-design-ai.html
Redirect 301 /contraco_pricing_psychology.html /pricing-psychology.html
Redirect 301 /contraco_resonance_method.html /resonance-method.html
Redirect 301 /contraco_strategic_intelligence.html /strategic-intelligence.html
Redirect 301 /contraco_thank-you.html /thank-you.html

# Force HTTPS
RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

# Force www removal (optional - adjust based on your preference)
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [R=301,L]
